<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 背景图 -->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/fhd_5"/>
            <solid android:color="@color/bottom_play_menu_progress_normal_bg"/>
        </shape>
    </item>
    <!-- 第二进度图 -->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/fhd_5"/>
                <solid android:color="@color/bottom_play_menu_progress_second_bg"/>
            </shape>
        </clip>
    </item>
    <!-- 进度度 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/fhd_5"/>
                <solid android:color="@color/bottom_play_menu_progress_bg"/>
            </shape>
        </clip>
    </item>
</layer-list>